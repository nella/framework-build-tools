<?xml version="1.0" encoding="UTF-8"?>
<phpunit>
	<testsuites>
		<testsuite name="FSHL">
			<directory>../tests/FSHL</directory>
		</testsuite>
	</testsuites>
	<filter>
		<whitelist addUncoveredFilesFromWhitelist="true">
			<directory suffix=".php">../FSHL</directory>
		</whitelist>
	</filter>
	<logging>
		<log type="coverage-html" target="../build/coverage" title="FSHL" charset="UTF-8" yui="true" highlight="true" lowUpperBound="35" highLowerBound="70" />
		<log type="coverage-clover" target="../build/logs/clover.xml" />
		<log type="junit" target="../build/logs/junit.xml" logIncompleteSkipped="false" />
	</logging>
</phpunit>