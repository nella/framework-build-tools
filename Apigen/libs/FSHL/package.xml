<name>FSHL</name>
<channel>pear.kukulich.cz</channel>
<summary>Fast Syntax HighLighter.</summary>
<description>
	FSHL is a free, open source, universal, fast syntax highlighter written in PHP. A very fast parser performs syntax highlighting for few languages and produces a HTML output.
</description>
<lead>
	<name>Jaroslav Hansl√≠k</name>
	<user>kukulich</user>
	<email>kontakt@kukulich.cz</email>
	<active>yes</active>
</lead>
<date>2011-08-29</date>
<version>
	<release>2.0.0</release>
	<api>2.0.0</api>
</version>
<stability>
	<release>stable</release>
	<api>stable</api>
</stability>
<license uri="http://fshl.kukulich.cz/#license">GPL v2+</license>
<notes>-</notes>
<contents>
	<dir name="/">
		<dir name="FSHL">
			<dir name="Lexer">
				<dir name="Cache">
					<file name="Cpp.php" role="php" />
					<file name="Css.php" role="php" />
					<file name="Html.php" role="php" />
					<file name="HtmlOnly.php" role="php" />
					<file name="Java.php" role="php" />
					<file name="Javascript.php" role="php" />
					<file name="Minimal.php" role="php" />
					<file name="Php.php" role="php" />
					<file name="Python.php" role="php" />
					<file name="Sql.php" role="php" />
					<file name="Texy.php" role="php" />
				</dir>
				<file name="Cpp.php" role="php" />
				<file name="Css.php" role="php" />
				<file name="Html.php" role="php" />
				<file name="HtmlOnly.php" role="php" />
				<file name="Java.php" role="php" />
				<file name="Javascript.php" role="php" />
				<file name="Minimal.php" role="php" />
				<file name="Php.php" role="php" />
				<file name="Python.php" role="php" />
				<file name="Sql.php" role="php" />
				<file name="Texy.php" role="php" />
			</dir>
			<dir name="Output">
				<file name="Html.php" role="php" />
				<file name="HtmlManual.php" role="php" />
			</dir>
			<file name="Generator.php" role="php" />
			<file name="Highlighter.php" role="php" />
			<file name="Lexer.php" role="php" />
			<file name="Output.php" role="php" />
		</dir>
		<file name="style.css" role="data" />
		<file name="LICENSE" role="doc" />
		<file name="README.md" role="doc" />
	</dir>
</contents>
<dependencies>
	<required>
		<php>
			<min>5.3.0</min>
		</php>
		<pearinstaller>
			<min>1.4.0</min>
		</pearinstaller>
	</required>
</dependencies>
<phprelease />
